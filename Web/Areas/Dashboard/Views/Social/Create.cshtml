@model Social


<div class="card">
    <form method="post">
        <div class="card-body">
            <div class="form-group">
                <input asp-for="Icon" class="form-control" placeholder="Icon"/>
            </div>
            <div class="form-group">
                <input asp-for="BaseUrl" class="form-control" placeholder="Url" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success" />
            </div>
        </div>
    </form>
</div>